{
  "name": "vite-plugin-vue-devtools",
  "type": "module",
  "version": "1.0.0-rc.3",
  "description": "A vite plugin for Vue DevTools",
  "author": "webfansplz",
  "license": "MIT",
  "homepage": "https://github.com/webfansplz/vue-devtools-next#readme",
  "repository": {
    "directory": "packages/vite",
    "type": "git",
    "url": "git+https://github.com/webfansplz/vue-devtools-next.git"
  },
  "bugs": {
    "url": "https://github.com/webfansplz/vue-devtools-next/issues"
  },
  "keywords": [
    "vue-devtools",
    "vite-plugin",
    "vite-plugin-vue-devtools",
    "dx"
  ],
  "exports": {
    ".": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/vite.cjs",
  "module": "dist/vite.mjs",
  "types": "dist/vite.d.ts",
  "files": [
    "*.d.ts",
    "./src/overlay.js",
    "dist",
    "overlay"
  ],
  "engines": {
    "node": ">=v14.21.3"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "vite": "^3.1.0 || ^4.0.0-0"
  },
  "dependencies": {
    "@vue-devtools-next/core": "workspace:^",
    "@vue-devtools-next/shared": "workspace:^",
    "@webfansplz/vuedoc-parser": "^0.0.4",
    "birpc": "^0.2.14",
    "execa": "^8.0.1",
    "sirv": "^2.0.3",
    "vite-plugin-inspect": "^0.7.42",
    "vite-plugin-vue-inspector": "^4.0.0",
    "vue-devtools-kit": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.9.4",
    "fast-glob": "^3.3.2",
    "image-meta": "^0.2.0",
    "pathe": "^1.1.1"
  }
}
