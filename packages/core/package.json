{
  "name": "@vue-devtools-next/core",
  "type": "module",
  "version": "0.0.1",
  "author": "webfansplz",
  "license": "MIT",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "git+https://github.com/webfansplz/vue-devtools-next.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean",
    "prepare:type": "tsup --dts-only",
    "prepublishOnly": "pnpm build",
    "stub": "tsup --watch --onSuccess 'tsup --dts-only'"
  },
  "dependencies": {
    "@vue-devtools-next/schema": "workspace:^",
    "@vue-devtools-next/shared": "workspace:^",
    "mitt": "^3.0.1",
    "vue-devtools-kit": "workspace:^"
  },
  "devDependencies": {
    "vue": "^3.3.5"
  }
}
