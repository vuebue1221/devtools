{
  "name": "vue-devtools-kit",
  "type": "module",
  "version": "0.0.1-aplha.0",
  "author": "webfansplz",
  "license": "MIT",
  "repository": {
    "directory": "packages/devtools-kit",
    "type": "git",
    "url": "git+https://github.com/webfansplz/vue-devtools-next.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.mjs",
      "require": "./dist/shared/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean",
    "prepare:type": "tsup --dts-only",
    "prepublishOnly": "pnpm build",
    "stub": "tsup --watch --onSuccess 'tsup --dts-only'"
  },
  "dependencies": {
    "@vue-devtools-next/schema": "workspace:^",
    "@vue-devtools-next/shared": "workspace:^",
    "hookable": "^5.5.3",
    "mitt": "^3.0.1",
    "perfect-debounce": "^1.0.0",
    "speakingurl": "^14.0.1"
  },
  "devDependencies": {
    "vue": "^3.3.7",
    "vue-router": "^4.2.5"
  }
}
