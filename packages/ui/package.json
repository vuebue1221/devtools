      "require": "./dist/unocss.cjs"
    },
    "./components/*": "./dist/components/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
