    "focus-trap": "^7.5.2",
    "unocss": "^0.55.3"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.17.1",
    "@vitejs/plugin-vue": "^4.3.3",
    "histoire": "^0.17.0",
    "unplugin": "^1.4.0",
    "unplugin-vue-components": "^0.25.1",
    "vite-plugin-dts": "^3.5.3"
  }
}
