{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "stub": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "prepare:type": {
      "outputs": ["dist/**/*.d.ts"]
    },
    "@vue/devtools-electron#build": {
      "dependsOn": ["@vue/devtools-client#build"],
      "outputs": ["dist/{app|cli|index|devtools|user-app}.?{c|m}js"]
    },
    "@ue/browser-extension#build": {
      "dependsOn": ["@vue/devtools-client#build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*.?{c|m}js"]
    }
  }
}
