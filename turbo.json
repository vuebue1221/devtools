{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "stub": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": true,
      "persistent": false
    },
    "prepare:type": {
      "outputs": ["dist/**/*.d.ts"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*.?{c|m}js"]
    }
  }
}
