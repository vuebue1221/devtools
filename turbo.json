{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "@vue/devtools#stub": {
      "cache": false,
      "dependsOn": [
        "@vue/devtools-electron#stub"
      ]
    },
    "@vue/devtools-browser-extension#build": {
      "dependsOn": [
        "@vue/devtools-client#build",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@vue/devtools-client#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "../electron/client/**",
        "../browser-extension/client/**",
        "../vite/client/**"
      ]
    },
    "@vue/devtools-electron#build": {
      "dependsOn": [
        "@vue/devtools-client#build",
        "^build"
      ],
      "outputs": [
        "dist/{app|cli|index|devtools|user-app}.?{c|m}js",
        "dist/{app|cli|index|devtools|user-app}.d.ts"
      ]
    },
    "@vue/devtools-electron#stub": {
      "cache": false,
      "dependsOn": [
        "@vue/devtools-client#stub",
        "^stub"
      ]
    },
    "@vue/devtools-ui#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/index.?{c|m}js",
        "dist/theme.?{c|m}js",
        "dist/types/**/*.d.ts"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*.?{c|m}js",
        "dist/**/*.d.ts"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "prepare:type": {
      "dependsOn": [
        "^prepare:type"
      ],
      "outputs": [
        "dist/**/*.d.ts"
      ]
    },
    "stub": {
      "cache": false
    },
    "vite-plugin-vue-devtools#build": {
      "dependsOn": [
        "@vue/devtools-client#build",
        "^build"
      ],
      "outputs": [
        "dist/**/*.d.ts",
        "dist/**/*.?{c|m}js"
      ]
    }
  }
}
